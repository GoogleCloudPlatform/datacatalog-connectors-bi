	"item": [
		{
			"name": "0 Authentication",
			"item": [
				{
					"name": "0.01.01 Sign in",
					"event": [
						{
							"listen": "prerequest",
							"script": {
								"id": "10d745db-cee3-4079-8841-4b5307ddfd41",
								"exec": [
									"pm.globals.clear();"
								],
								"type": "text/javascript"
							}
						},
						{
							"listen": "test",
							"script": {
								"id": "855aa168-0e50-4eae-b20b-658b69c83159",
								"exec": [
									"pm.globals.set(\"siteId\", pm.response.json().credentials.site.id);",
									"pm.globals.set(\"userId\", pm.response.json().credentials.user.id);",
									"pm.globals.set(\"token\", pm.response.json().credentials.token);"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"name": "Content-Type",
								"value": "application/json",
								"type": "text"
							},
							{
								"key": "Accept",
								"value": "application/json",
								"type": "text"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n\t\"credentials\": {\n\t\t\"name\": \"{{USERNAME}}\",\n\t\t\"password\": \"{{PASSWORD}}\",\n\t\t\"site\": {\n\t\t\t\"contentUrl\": \"{{SITE_URL}}\"\t\n\t\t}\n\t}\n}"
						},
						"url": {
							"raw": "{{PROTOCOL}}://{{SERVER}}/api/{{API_VERSION}}/auth/signin",
							"protocol": "{{PROTOCOL}}",
							"host": [
								"{{SERVER}}"
							],
							"path": [
								"api",
								"{{API_VERSION}}",
								"auth",
								"signin"
							]
						}
					},
					"response": []
				}
			],
			"protocolProfileBehavior": {}
		},
		{
			"name": "1 REST API",
			"item": [
				{
					"name": "1.01.01 Get Databases for site",
					"event": [
						{
							"listen": "test",
							"script": {
								"id": "e0d2d0e2-e882-46a2-9aed-ccc8bbfdff7e",
								"exec": [
									"const jsonData = pm.response.json();",
									"const databases = jsonData.databases.database;",
									"",
									"pm.globals.unset(\"databaseId\");",
									"",
									"if (databases.length > 0) {",
									"    pm.globals.set(\"databaseId\", databases[0].id);",
									"}",
									""
								],
								"type": "text/javascript"
							}
						}
					],
					"protocolProfileBehavior": {
						"disableBodyPruning": true
					},
					"request": {
						"method": "GET",
						"header": [
							{
								"key": "X-Tableau-Auth",
								"type": "text",
								"value": "{{token}}"
							},
							{
								"key": "Content-Type",
								"name": "Content-Type",
								"type": "text",
								"value": "application/json"
							},
							{
								"key": "Accept",
								"value": "application/json",
								"type": "text"
							}
						],
						"body": {
							"mode": "raw",
							"raw": ""
						},
						"url": {
							"raw": "{{PROTOCOL}}://{{SERVER}}/api/{{API_VERSION}}/sites/{{siteId}}/databases",
							"protocol": "{{PROTOCOL}}",
							"host": [
								"{{SERVER}}"
							],
							"path": [
								"api",
								"{{API_VERSION}}",
								"sites",
								"{{siteId}}",
								"databases"
							]
						}
					},
					"response": []
				},
