				{
					"name": "0.01.01 Sign in",
					"event": [
						{
							"listen": "prerequest",
							"script": {
								"id": "9d9dd1a1-fb2c-4232-b58b-b7449178b232",
								"exec": [
									"pm.globals.clear();"
								],
								"type": "text/javascript"
							}
						},
						{
							"listen": "test",
							"script": {
								"id": "8539e066-8311-4393-ad8b-8bbcbfc4cfbf",
								"exec": [
									"pm.globals.set(\"siteId\", pm.response.json().credentials.site.id);",
									"pm.globals.set(\"userId\", pm.response.json().credentials.user.id);",
									"pm.globals.set(\"token\", pm.response.json().credentials.token);",
									""
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"name": "Content-Type",
								"value": "application/json",
								"type": "text"
							},
							{
								"key": "Accept",
								"value": "application/json",
								"type": "text"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n\t\"credentials\": {\n\t\t\"name\": \"{{USERNAME}}\",\n\t\t\"password\": \"{{PASSWORD}}\",\n\t\t\"site\": {\n\t\t\t\"contentUrl\": \"{{SITE_URL}}\"\t\n\t\t}\n\t}\n}"
						},
						"url": {
							"raw": "{{PROTOCOL}}://{{SERVER}}/api/{{API_VERSION}}/auth/signin",
							"protocol": "{{PROTOCOL}}",
							"host": [
								"{{SERVER}}"
							],
							"path": [
								"api",
								"{{API_VERSION}}",
								"auth",
								"signin"
							]
						}
					},
					"response": []
				}
			],
			"protocolProfileBehavior": {}
		},
